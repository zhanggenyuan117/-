.changjia{
  width:750rpx;
  height:140rpx;
  background:rgba(247,247,247,1);
}
.changjiaView{
  width: 690rpx;
  height: 60rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}
.changjiaView input{
  width: 300rpx;
  height: 60rpx;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
  margin-top: 40rpx;
  font-size:30rpx;
  padding-left: 10rpx;
  padding-right: 50rpx;
}
.changjiaText{
  margin-top: 49rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(13,13,13,1);
  width: 30vw;
}
.scrollView{
  height: calc(100vh - 140rpx)
}
.forView{
  padding: 20rpx;
  border: 1rpx solid rgba(247,247,247,1);
  font-size: 30rpx;
  font-family:PingFang SC;
}
.searchIcon{
  width: 30rpx;
  height: 30rpx;
}
.viewBox{
  width: 70vw;
  display: flex;
  justify-content: space-between;
}
.searchview{
  width: 60rpx;
  height: 60rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 25rpx;
  margin-top: 40rpx;
}

