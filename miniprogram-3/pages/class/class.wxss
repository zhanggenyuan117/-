/* 筛选 */

.popup{
  width: 70vw;
  height: 100vh;
  background: #fff;
  overflow: hidden;
  /* color: #333; */
}
/* 输入框 */
.popupInputView{
  padding: 30rpx 10rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.popupInput{
  width: 60vw;
  height: 44rpx;
  border: 1rpx solid #333;
  border-radius: 5rpx;
  margin-left: 5vw;
  margin-top: 30rpx;
}
/* 两个按钮 */
.popupBtns{
  position: fixed;
  bottom: 0rpx;
  display: flex;
  justify-content: space-between;
  width: 70vw;
}
.popupBtns button{
  background: #fff;
  color: #333;
  width: 50%;
  border-radius: 0;
}
.popupBtns button:last-of-type{
  color: #fff;
  background-color: #5ba2f3;
}

