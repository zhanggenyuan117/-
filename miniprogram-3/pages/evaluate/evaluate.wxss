

page {
  width: 100%;
  height: 100%;
  background: #f0f0f0;
}
.service {
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  padding: 0 20rpx;
  background: #fff;
}
.score {
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid #D9D9D9;
}
.star {
  float: left;
  width: 54rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}
.star image{
  width: 40rpx;
  height: 40rpx;
  margin-top: 30rpx;
}
.score .left {
  width: 220rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  float: left;
}
.score .right {
  width: 350rpx;
  font-size: 30rpx;
  float: left;
  color: #888;
}

.score .right .rtxt {
  display: inline-block;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 5rpx;
  color: #F82525;
  font-weight: bold;
  font-family:PingFang SC;
}

.labelColor {
  color: #F82525;
  border: 1px solid #F82525 !important;
}
.labelColor1 {
  color: #B8B8B8;
  border: 1px solid #B8B8B8 !important;
}
.content {
  width: 100%;
  overflow: hidden;
  border-bottom:1px solid #ccc;
  padding:0 0 26rpx 0;
  box-sizing: border-box;
} 
.content .left {
  width: 100rpx;
  font-size: 30rpx;
  float: left;
  padding-top: 20rpx;
}
.content .right {
  width: 610rpx;
  font-size: 24rpx;
  float: left;
  color: #888;
}
.content .right text {
  font-size: 24rpx;
  padding: 13rpx 24rpx;
  float: left;
  border: 1px solid #888;
  border-radius: 10rpx;
  margin-right: 34rpx;
  margin-top: 20rpx;
}
.picture {
  width: 100%;
  overflow: hidden;
  background: #fff;
  border-bottom: 1px solid #ccc;
  display: flex;
}
.picture .left {
  width: 100rpx;
  padding-top: 20rpx;
}
.picture .right {
  width: 610rpx;
}
.picture .right .add {
  width: 25%;
  height: 160rpx;
}
.parcelBox{
  display: flex;
  flex-wrap: wrap;
}
.parcel{
  width: 25%;
  height: 160rpx;
  box-sizing: border-box;
  padding: 10rpx;
}
.According{
  width: 100%;
  height: 100%;
}
.deleteimg{
  width:30rpx;
  height:30rpx;
  position: absolute;
  right:0;
  top:0;
  z-index: 2;
}
.conts{
  width: 92%;
  height: 260rpx;
  background: #fff;
  border: 1rpx solid #dbdbdb;
 margin-left: 30rpx;
}
.contsb{
  width: 100%;
  height: 380rpx;
  background: #fff;
  margin-top: -1rpx;
}
textarea {
  width: 687rpx;
}
.areas{
  width:687rpx;
  height:315rpx;
  font-size: 30rpx;
  padding-top: 10rpx;
  padding-left: 10rpx;
  margin: 0 auto;
  overflow: hidden;   
  position: relative; 
}
.currentWordNumber{
  font-size: 28rpx;
  position: absolute;
  left: 0%;
  bottom: 0rpx;
  color: #c00;
}
.hint{
   font-size: 28rpx;
   position: absolute;
   top: 120rpx;
   left: 30rpx;
   color: #FF6600;
}
.contsa{
  width: 100%;
  height: 100rpx;
  background: #fff;
}
.btn {
  width: 600rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #fff;
  background: #3E7EE9;
  position: fixed;
  left:75rpx;
  bottom: 37rpx;
} 
button:after {
  border: 0;
}
.selectCountStyle{
  border-color: #F82525 !important;
  color: #F82525;
}