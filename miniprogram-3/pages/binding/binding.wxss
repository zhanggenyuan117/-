
.changjia{
  width:750rpx;
  height:140rpx;
  background:rgba(247,247,247,1);
  position: relative;
}
.changjiaView{
  width: 690rpx;
  height: 60rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}
.changjiaView input{
  width: 470rpx;
  height: 60rpx;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
  margin-top: 40rpx;
  font-size:30rpx;
  padding-left: 10rpx;
  padding-right: 50rpx;
}
.changjiaText{
  margin-top: 49rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(13,13,13,1);
  width: 190rpx;
}
.kehu{
  margin-top: 20rpx;
}
.scrollView{
  margin-top: 20rpx;
  height: calc(100vh - 340rpx - 56px);
}
.radio{
  width:750rpx;
  /* height:268rpx; */
  background:rgba(247,247,247,1);
  padding: 30rpx;
  margin-top: 20rpx;
}
.xiala{
  /* margin-top: 30rpx; */
  width:630rpx;
  /* height:208rpx; */
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(255,255,255,1);
  border-radius:15px;
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  justify-content: space-around;
}
.xialaView{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  padding: 20rpx 0;
}
.xialatext{
  width:150rpx;
  /* height:42rpx; */
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(114,114,114,1);
}
.xialaDong{
  width:336rpx;
  /* height:40rpx; */
  font-size:28rpx;
  font-family:PingFang SC;
  color:rgba(13,13,13,1);
  display: flex;
  justify-content: flex-end;
  /* overflow: hidden; */
  flex-wrap: wrap;
  text-align: end;
}
/* 绑定按钮 */
.btns{
  width: 100%;
  height: 56px;
  position: fixed;
  bottom: 0rpx;
  /* padding-bottom: 20rpx; */
  background: #fff;
  z-index: 999;
  overflow-x: hidden;
}
.commit{
  background: #5ba2f3;
  color: #fff;
  width: 90vw;
  margin-left: 5vw;
}
/* 添加搜索图标 */
.search{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 20rpx;
  top: 30rpx;
}
.kehuViewBox{
  display: flex;
  width:570rpx;
  /* justify-content: space-between; */
}
.kehuInput{
  width: 400rpx !important;
}
.kehuView{
  right: 0; 
  top: 40rpx;
  width: 100rpx;
  height: 60rpx;
  background-color:#fff;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 15rpx;
}
.searchIcon{
  width: 30rpx;
  height: 30rpx;
}
.searchInput{
  padding-right: 0 !important;
}
.kehuText{
  width: 160rpx;
}
.kehuViewFaze{
  display: flex;
  
}